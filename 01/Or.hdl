/**
 * Or gate:
 * if ((a == 1) or (b == 1)) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    Not(in=a, out=na);
    Not(in=b, out=nb);
    And(a=na, b=nb, out=nanb);
    Not(in=nanb, out= out);


}
